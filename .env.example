# Docker Compose Environment Variables for Magento 2.3.4

# MySQL Configuration
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=magento
MYSQL_USER=magento
MYSQL_PASSWORD=magento
MYSQL_PORT=3306

# PHP Configuration
PHP_MEMORY_LIMIT=2048M
PHP_MAX_EXECUTION_TIME=18000
PHP_UPLOAD_MAX_FILESIZE=128M
PHP_POST_MAX_SIZE=128M

# Nginx Configuration
NGINX_PORT=80
NGINX_SSL_PORT=443

# Application Settings
APP_DEBUG=true
APP_ENV=development
APP_TIMEZONE=UTC
